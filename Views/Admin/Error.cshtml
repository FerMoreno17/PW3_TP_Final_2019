@{
    Layout = "../Shared/_LayoutAdmin.cshtml";
    ViewBag.Title = "Error";
}


<section class="wrapper mt-0 h-100">
    <div class="row content-panel h-100 justify-content-center">
        <div class="col-lg-8 detailed">
            <h4 class="mb">Advertencia</h4>
            <div class="alert alert-danger">
                <strong>Lo sentimos!</strong> @ViewBag.MotivoError.
            </div>
            <div class="form-group">
                <a class="btn btn-info" href="@ViewBag.Volver" >Volver</a>
            </div>
        </div>
    </div>
</section>